CREATE TABLE players
(
  player_id INTEGER PRIMARY KEY,
  first_name TEXT,
  last_name  TEXT,
  age INTEGER
);

CREATE TABLE teams
(
	team_id INTEGER PRIMARY KEY,
  	team_name TEXT,
  	team_location TEXT,
  	team_number_of_players INTEGER
);

